<table>
  <thead>
    <th>Player</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Occupation</th>
    <th>Degrees</th>
    <th>Birthplace</th>
    <th>Gender</th>
    <th>Age</th>
    <th></th>
  </thead>

  <% characters.each do |character| %>
    <tr>
      <td><%= character.user.display_name %></td>
      <td><%= character.first_name %></td>
      <td><%= character.last_name %></td>
      <td><%= character.occupation %></td>
      <td><%= character.degrees %></td>
      <td><%= character.birthplace %></td>
      <td><%= character.gender %></td>
      <td><%= character.age %></td>
      <td>
        <%= link_to 'View', coc_character_path( character ), method: :get %>
      </td>
    </tr>
  <% end %>
</table>
